<% for(var i=0;i<posts.length;i++) { %>
	<div class="post">
		<div class="header">
			<h3><a href="<%= posts[i].permalink %>"><%= posts[i].title %></a></h3>
		</div>
		<div class="body">
			<%= posts[i].content %>
		</div>
  		<div class="footer"><span><%= posts[i].created.toString() %></span></div>
	</div>
<% } %>